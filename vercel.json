{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "fastapi",
  "buildCommand": "pip install -r requirements.txt",
  "devCommand": "cd src && uvicorn app:app --host 0.0.0.0 --port 3000",
  "installCommand": "pip install -r requirements.txt",
  "regions": ["iad1"],
  "functions": {
    "src/**/*.py": {
      "runtime": "python3.11",
      "maxDuration": 60
    }
  },
  "env": {
    "PYTHONUTF8": "1"
  }
}
